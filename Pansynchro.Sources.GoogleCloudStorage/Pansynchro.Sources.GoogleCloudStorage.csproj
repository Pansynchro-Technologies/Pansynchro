<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFrameworks>net6.0;net6.0-windows;net7.0;net7.0-windows</TargetFrameworks>
	<ImplicitUsings>enable</ImplicitUsings>
	<Nullable>enable</Nullable>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<AssemblyVersion>0.6.0</AssemblyVersion>
	<FileVersion>0.6.0</FileVersion>
	<Version>0.6.0</Version>
	<Authors>Pansynchro Technologies</Authors>
	<Product>Pansynchro Google Cloud Storage Data Source</Product>
	<Description>Pansynchro data source to read from and write to GCS.</Description>
	<Copyright>Copyright 2022, 2023 Pansynchro Technologies</Copyright>
	<PackageProjectUrl>https://github.com/Pansynchro-Technologies/Pansynchro</PackageProjectUrl>
	<PackageTags>pansynchro;data;data-integration;etl;elt;gcs</PackageTags>
	<PackageReleaseNotes>Updating dependencies.</PackageReleaseNotes>
	<PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
	<PackageReference Include="Google.Cloud.Storage.V1" Version="4.6.0" />
	<PackageReference Include="System.IO.Pipelines" Version="7.0.0" />
  </ItemGroup>

  <ItemGroup>
	<ProjectReference Include="..\Pansynchro.core\Pansynchro.Core.csproj" />
  </ItemGroup>

</Project>
