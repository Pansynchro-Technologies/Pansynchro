<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<AssemblyVersion>0.3.0</AssemblyVersion>
		<FileVersion>0.3.0</FileVersion>
		<Version>0.3.0</Version>
		<Authors>Pansynchro Technologies</Authors>
		<Product>PanSQL core runtime</Product>
		<Description>Supports data pipelines built with PanSQL</Description>
		<Copyright>Copyright 2023-2025, Pansynchro Technologies</Copyright>
		<PackageProjectUrl>https://github.com/Pansynchro-Technologies/Pansynchro</PackageProjectUrl>
		<PackageTags>pansynchro;data;data-integration;etl;elt</PackageTags>
		<PackageReleaseNotes>
			Updating to use the new type system
			Adding new SQL functions
		</PackageReleaseNotes>
		<PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
		<NoWarn>$(NoWarn);NU1605</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="JsonPath.Net" Version="2.1.1" />
		<PackageReference Include="Pansynchro.Core" Version="0.7.0" />
		<PackageReference Include="System.IO.Compression" Version="4.3.0" />
		<PackageReference Include="System.IO.Hashing" Version="9.0.7" />
	</ItemGroup>

</Project>
